---
import favIcon from '~/assets/favicons/custom-f-logo.svg';
import favIconSvg from '~/assets/favicons/custom-f-logo.svg';
import appleTouchIcon from '~/assets/favicons/custom-f-logo.svg';
---

<link rel="shortcut icon" href={favIcon.src} />
<link rel="icon" type="image/svg+xml" href={favIconSvg.src} />
<link rel="mask-icon" href={favIconSvg.src} color="var(--aw-color-primary)" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
